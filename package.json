{
  "name": "starwars-react",
  "version": "1.0.0",
  "description": "This is an assignment to show star wars characters",
  "main": "dist/app.js",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "npx stylelint \"**/*.scss\" --color",
    "lint:js": "npx eslint . --ext .jsx,.js --color --no-error-on-unmatched-pattern",
    "pretest": "npm run lint",
    "test": "npx cross-env NODE_ENV=test npx jest --coverage --verbose",
    "test:watch": "npx cross-env NODE_ENV=test npx jest --watchAll",
    "clean": "npm run clean:build",
    "clean:build": "npx rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "npx webpack --config ./internals/webpack/webpack.build.js",
    "build:prod": "npx cross-env NODE_ENV=prod npm run build",
    "build:dev": "npx cross-env NODE_ENV=dev npm run build -- --colors --progress",
    "start": "npx webpack-dev-server --config ./internals/webpack/webpack.serve.js --colors --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedro-home/starwars-react.git"
  },
  "keywords": [
    "react",
    "front-end",
    "js",
    "css",
    "scss"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "maintained node versions"
  ],
  "author": "Pedro Gomes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedro-home/starwars-react/issues"
  },
  "homepage": "https://github.com/pedro-home/starwars-react",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/react": "^9.5.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "jest-dom": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@babel/runtime-corejs3": "^7.8.7",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "dots.js": "0.0.6"
  }
}
